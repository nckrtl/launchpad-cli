{
    "main": "launchpad",
    "output": "builds/launchpad.phar",
    "chmod": "0755",
    "directories": [
        "app",
        "bootstrap",
        "config",
        "stubs",
        "vendor",
        "web"
    ],
    "files": [
        "composer.json"
    ],
    "exclude-composer-files": false,
    "compression": "GZ",
    "compactors": [
        "KevinGH\\Box\\Compactor\\Php",
        "KevinGH\\Box\\Compactor\\Json"
    ],
    "blacklist": [
        "web/vendor",
        "web/node_modules",
        "web/.env",
        "web/storage/logs/*",
        "web/storage/framework/cache/*",
        "web/storage/framework/sessions/*",
        "web/storage/framework/views/*",
        "tests"
    ],
    "check-requirements": false
}
