{
    "main": "orbit",
    "output": "builds/orbit.phar",
    "chmod": "0755",
    "directories": [
        "app",
        "bootstrap",
        "config",
        "stubs",
        "vendor",
        "web"
    ],
    "files": [
        "composer.json"
    ],
    "exclude-composer-files": false,
    "compression": "NONE",
    "compactors": [
        {
            "class": "KevinGH\\Box\\Compactor\\Php",
            "extensions": ["php"]
        },
        {
            "class": "KevinGH\\Box\\Compactor\\Json",
            "extensions": ["json", "lock"]
        }
    ],
    "blacklist": [
        "web/vendor",
        "web/node_modules",
        "web/.env",
        "web/storage/logs/*",
        "web/storage/framework/cache/*",
        "web/storage/framework/sessions/*",
        "web/storage/framework/views/*",
        "tests"
    ],
    "check-requirements": false
}
