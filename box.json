{
    "main": "orbit",
    "output": "builds/orbit.phar",
    "chmod": "0755",
    "directories": [
        "app",
        "bootstrap",
        "config",
        "stubs",
        "vendor"
    ],
    "files": [
        "composer.json"
    ],
    "exclude-composer-files": false,
    "compression": "NONE",
    "compactors": [
        {
            "class": "KevinGH\\Box\\Compactor\\Php",
            "extensions": ["php"]
        },
        {
            "class": "KevinGH\\Box\\Compactor\\Json",
            "extensions": ["json", "lock"]
        }
    ],
    "blacklist": [
        "tests"
    ],
    "check-requirements": false
}
